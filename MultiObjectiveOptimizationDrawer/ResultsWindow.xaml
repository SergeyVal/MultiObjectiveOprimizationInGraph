<Window x:Class="MultiObjectiveOptimizationDrawer.ResultsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ResultsWindow" Height="300" Width="300">
    <Grid>
        <Menu>
            <MenuItem Header="File">
                <MenuItem Header="ClearCanvas" Name="ClearMenuItem" Click="ClearMenuItem_Click"></MenuItem>
            </MenuItem>
        </Menu>
        <Canvas x:Name="ResultsCanvas" Margin="5,20,10,10" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="White">
        </Canvas>
    </Grid>
</Window>
